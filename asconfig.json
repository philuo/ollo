{
  "entries": [
    "src/ollo/assembly/index"
  ],
  "targets": {
    "release": {
      "outFile": "dist/rei.wasm",
      "textFile": "dist/rei.wat",
      "sourceMap": false,
      "optimizeLevel": 3,
      "shrinkLevel": 2,
      "noAssert": true,
      "runtime": "stub",
      "initialMemory": 8192,
      "maximumMemory": 8192,
      "sharedMemory": true,
      "importMemory": true,
      "memoryBase": 0
    },
    "debug": {
      "outFile": "src/ollo/dist/rei.debug.wasm",
      "textFile": "src/ollo/dist/rei.debug.wat",
      "sourceMap": true,
      "debug": true,
      "runtime": "stub",
      "initialMemory": 8192,
      "maximumMemory": 8192,
      "sharedMemory": true,
      "importMemory": true
    }
  },
  "options": {
    "bindings": "esm",
    "exportRuntime": false,
    "transform": [],
    "use": [
      "Math=NativeMath"
    ],
    "enable": [
      "simd",
      "threads",
      "bulk-memory",
      "sign-extension"
    ],
    "disable": [
      "mutable-globals"
    ],
    "exportTable": false,
    "exportStart": false
  }
}